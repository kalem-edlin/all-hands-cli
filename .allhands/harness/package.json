{
  "name": "allhands-cli",
  "version": "0.1.0",
  "private": true,
  "description": "Internal CLI for All Hands harness",
  "type": "module",
  "bin": {
    "ah": "./bin/ah"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run src/__tests__/e2e/",
    "test:e2e:verbose": "vitest run src/__tests__/e2e/ --reporter=verbose",
    "test:unit": "vitest run src/lib/__tests__/ src/mcp/__tests__/"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/blessed": "^0.1.25",
    "@types/minimatch": "^5.0.0",
    "@types/node": "^20.10.0",
    "@types/proper-lockfile": "^4.1.4",
    "tsx": "^4.19.0",
    "typescript": "^5.3.0",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@opencode-ai/sdk": "^1.1.14",
    "@upstash/context7-sdk": "^0.3.0",
    "@visheratin/tokenizers-node": "0.1.5",
    "@visheratin/web-ai-node": "^1.4.5",
    "better-sqlite3": "^11.0.0",
    "blessed": "^0.1.81",
    "commander": "^12.0.0",
    "gray-matter": "^4.0.3",
    "minimatch": "^9.0.0",
    "node-fetch": "^3.3.0",
    "onnxruntime-node": "1.15.1",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "proper-lockfile": "^4.1.2",
    "usearch": "^2.21.4",
    "yaml": "^2.3.0",
    "zod": "^3.22.0"
  },
  "engines": {
    "node": ">=20"
  }
}
